<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.com.WCZZ.mapper.RoleMapper">
    <select id="queryByUserId" parameterType="integer" resultType="main.com.WCZZ.entity.Role">
        select * from role where role_id in (select role_id from r_user_role where user_id=#{userId})
    </select>
</mapper>