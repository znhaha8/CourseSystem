<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.com.WCZZ.mapper.PermissionMapper">
    <select id="selectPermissionByRoleId" resultType="main.com.WCZZ.entity.Permission">
        SELECT
          *
        FROM permission
        WHERE
        role_id = #{roleId}
    </select>
</mapper>